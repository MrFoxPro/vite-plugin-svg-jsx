{
  "name": "vite-plugin-svg-jsx",
  "type": "module",
  "version": "0.1.0",
  "description": "Import SVG files as JSX components",
  "keywords": [
    "vite-plugin",
    "svg",
    "svgo",
    "vite"
  ],
  "bugs": "https://github.com/mrfoxpro/vite-plugin-svg-jsx/issues",
  "repository": "https://github.com/mrfoxpro/vite-plugin-svg-jsx",
  "license": "MIT",
  "author": "mrfoxpro <tuningiposadka@gmail.com>",
  "exports": {
    "./types": {
      "types": "./types.d.ts"
    },
    "./types-jsx-svg": {
      "types": "./types-jsx-svg.d.ts"
    },
    ".": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "watch": "rollup -c -w",
    "build": "rollup -c && tsc"
  },
  "files": [
    "./dist",
    "./types.d.ts",
    "./jsx-component.d.ts"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "svgo": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-typescript": "^7.22.11",
    "@rollup/plugin-babel": "^6.0.3",
    "@types/svgo": "^3.0.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "rollup": "^3.28.1",
    "rollup-plugin-delete": "^2.0.0",
    "solid-js": "^1.7.11",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "peerDependencies": {
    "vite": ">=2.4"
  }
}
