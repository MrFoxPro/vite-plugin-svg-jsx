{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["vite/client", "../../types"],
    "noEmit": true,
    "isolatedModules": true,
    "paths": {
      "@/*": ["../common/*"]
    }
  }
}
